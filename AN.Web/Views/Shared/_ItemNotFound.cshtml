@model string

<div class="container p-5">
    <div class="row align-items-center">
        <div class="col p-5">
            <div class="text-center">
                <img src="~/images/no-found.png" style="max-width:450px;margin:30px auto;">                
            </div>
            <p class="text-center mt-5">
                @if (!string.IsNullOrEmpty(Model))
                {
                    <h4 class="text-center">@Model</h4>
                }
                else
                {
                    <h4 class="text-center">@AN.Core.Resources.Global.Global.NoItemFound</h4>
                }
            </p>             
        </div>
    </div>
</div>

