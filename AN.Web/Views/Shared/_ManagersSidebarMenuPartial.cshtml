@model (Shared.Enums.LoginAs loginAs, AN.Core.Models.WorkingAreaModel workingArea)

@if (Model.loginAs == Shared.Enums.LoginAs.ADMIN)
{
    @await Html.PartialAsync("~/Areas/Admin/Views/Shared/_AdminSidebarMenuPartial.cshtml")
}
else if (Model.loginAs == Shared.Enums.LoginAs.HOSPITALMANAGER)
{
    @await Html.PartialAsync("~/Areas/HospitalManager/Views/Shared/_HospitalManagerSidebarMenuPartial.cshtml", Model.workingArea.Id)
}
else if (Model.loginAs == Shared.Enums.LoginAs.CLINICMANAGER)
{
