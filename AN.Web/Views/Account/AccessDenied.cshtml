
@{
    ViewBag.Title = "Access Denied";
    Layout = "~/Views/Shared/_CpanelLayout.cshtml";
}

<section class="content p-4">
    <div class="card">      
        <div class="card-body">
            <div class="row align-items-center">               
                <div class="col">
                    <p class="text-center"><i class="fa fa-user-lock fa-5x text-warning"></i></p>
                    <h2 class="text-center mt-2">@AN.Core.Resources.Global.Global.AccessDenied</h2>
                    <p class="text-center mt-3 text-gray">@AN.Core.Resources.EntitiesResources.Messages.AccessDenied</p>
                </div>               
            </div>
        </div>      
    </div>
</section>
